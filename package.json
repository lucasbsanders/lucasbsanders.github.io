{
  "name": "ryan-portfolio-ui",
  "version": "0.2.1",
  "scripts": {
    "ng": "ng",
    "config": "npx ts-node ./src/assets/scripts/setEnv.ts",
    "start": "npm run config -- --environment=dev && ng serve",
    "start-prod": "npm run config -- --environment=prod && ng serve",
    "build-dev": "npm run config -- --environment=development && ng build",
    "build-prod": "npm run config -- --environment=prod && ng build --configuration=production --output-hashing=all",
    "deploy-dev": "npm run config -- --environment=prod && ng deploy --base-href=/ryan-portfolio-ui/",
    "deploy-prod": "npm run build-prod && aws s3 sync ./dist/ryan-portfolio-ui s3://ryanfennessey.com --delete --cache-control max-age=600",
    "watch": "ng build --watch --configuration development",
    "test": "npm run config -- --environment=dev && ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~16.1.1",
    "@angular/common": "~16.1.1",
    "@angular/compiler": "~16.1.1",
    "@angular/core": "~16.1.1",
    "@angular/forms": "~16.1.1",
    "@angular/platform-browser": "~16.1.1",
    "@angular/platform-browser-dynamic": "~16.1.1",
    "@angular/router": "~16.1.1",
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "md5": "^2.3.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "util": "^0.12.4",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.0",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "~16.1.1",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "angular-cli-ghpages": "^1.0.5",
    "dotenv": "^16.0.0",
    "eslint": "^8.39.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~5.1.3",
    "yargs": "^17.4.1"
  }
}